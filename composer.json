{
    "name": "get-tony/bal-kit",
    "version": "1.0.0",
    "description": "BAL Kit for Laravel - Bootstrap + Alpine + Livewire toolkit with professional UI components",
    "keywords": [
        "laravel",
        "bootstrap",
        "alpine",
        "livewire",
        "kit",
        "ui",
        "components",
        "starter"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Anthony Pagan",
            "email": "get-tony@outlook.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "symfony/process": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelBalKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelBalKit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelBalKit\\BalKitServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "homepage": "https://github.com/get-tony/bal-kit",
    "support": {
        "issues": "https://github.com/get-tony/bal-kit/issues",
        "source": "https://github.com/get-tony/bal-kit"
    }
}
